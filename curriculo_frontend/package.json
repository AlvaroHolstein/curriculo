    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^12.0.0",
    "socket.io-client": "^2.3.0",
